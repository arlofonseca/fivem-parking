    "@prisma/client": "^6.5.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@citizenfx/client": "latest",
    "@citizenfx/server": "latest",
    "@types/node": "^22.13.10",
    "esbuild": "^0.25.1",
    "esbuild-plugin-fileloc": "^0.0.6",
    "prisma": "^6.5.0"
