    "@prisma/client": "^5.21.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@citizenfx/client": "latest",
    "@citizenfx/server": "latest",
    "@types/node": "^22.7.5",
    "esbuild": "^0.25.0",
    "esbuild-plugin-fileloc": "^0.0.6",
    "prisma": "^5.21.1"
  }
